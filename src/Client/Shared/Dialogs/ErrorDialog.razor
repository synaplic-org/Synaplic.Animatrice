<MudDialog>
    <DialogContent>
        <MudText>Error</MudText>
    </DialogContent>
    <DialogContent>
        
        @foreach (var s in Errors)
        {
            <MudAlert Severity="Severity.Error">@s</MudAlert>
        }
        
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" Variant="Variant.Filled" OnClick="Submit">Close</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    [Parameter] public List<string> Errors { get; set; }
    
    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    
}